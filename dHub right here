coroutine.wrap(PMHWQ_fake_script)()
local function IHMRNML_fake_script() -- TPBallAutoParry.LocalScript 
	local script = Instance.new('LocalScript', TPBallAutoParry)

	script.Parent.MouseButton1Click:Connect(function()
		getgenv().ScriptConfig = {
			-- Distance in stud before the automatic triggers,
			-- you may change even after you have ran the script if you desire!
			-- Just be sure to REMOVE the loadstring, or else you will face some... issues
			DistanceBeforeParry = 48,
		}
		local plyrs = game.Players
		local plyr = plyrs.LocalPlayer
		local hum = plyr.Character:FindFirstChildOfClass("Humanoid")
		if hum.Health == 0.5 then
			hum.Health = 100
		end
		local LocalPlayer = game:GetService("Players").LocalPlayer
		game:GetService("Workspace").Balls.ChildAdded:Connect(function(child)
			local trackTask = task.spawn(function()
				local ball = child
				while task.wait() do
					if string.find(ball.Color.Name:lower() or ball.BrickColor.Name:lower(), "red") then
						plyr.Position = ball.CFrame.Position or ball.Position
						plyr.Character.PivotTo(ball.CFrame.Position)
						while LocalPlayer:DistanceFromCharacter(ball.CFrame.Position)
							> getgenv().ScriptConfig.DistanceBeforeParry do
							task.wait()
						end
						local ReplicatedStorage = game:GetService("ReplicatedStorage")
						ReplicatedStorage.Remotes.ParryButtonPress:Fire()
						ReplicatedStorage.Remotes.ParryButtonPress:Fire()
					end
				end
			end)
			child.Destroying:Connect(function()
				task.cancel(trackTask)
			end)
		end)
	end)
end
coroutine.wrap(IHMRNML_fake_script)()
local function IXKHB_fake_script() -- UserName.LocalScript 
	local script = Instance.new('LocalScript', UserName)

	script.Parent.Text = game.Players.LocalPlayer.Name
end
coroutine.wrap(IXKHB_fake_script)()
local function ARQUB_fake_script() -- hide.LocalScript 
	local script = Instance.new('LocalScript', hide)

	script.Parent.MouseButton1Click:Connect(function()
		local dhub = script.Parent.Parent.dHub
		if dhub.Visible == true then
			dhub.Visible = false
			script.Parent.Text = "Hide"
		else if dhub.Visible == false then
				dhub.Visible = true
				script.Parent.Text = "Show"
			end
		end
	end)
end
coroutine.wrap(ARQUB_fake_script)()
local function HRMPZM_fake_script() -- notif.anim_notif 
	local script = Instance.new('LocalScript', notif)

	local animation = game:GetService("TweenService")
	-- Animation Properties
	script.Parent:TweenPosition(UDim2.new(0.932, 0,0.755, 0), Enum.EasingDirection.In, Enum.EasingStyle.Bounce, 0.5)
	wait(5)
	script.Parent:TweenPosition(UDim2.new(1.2, 0,0.755, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, 0.5)
end
coroutine.wrap(HRMPZM_fake_script)()
