-- Scripts:

local function GJBB_fake_script() -- title.LocalScript 
	local script = Instance.new('LocalScript', title)

	while true do
		wait(1)
		script.Parent.Text = "ðŸŽƒDawgHubðŸŽƒ | Beta | E"
		wait(1)
		script.Parent.Text = "ðŸŽƒDawgHubðŸŽƒ | Beta | EA"
		wait(1)
		script.Parent.Text = "ðŸŽƒDawgHubðŸŽƒ | Beta | EAS"
		wait(1)
		script.Parent.Text = "ðŸŽƒDawgHubðŸŽƒ | Beta | EASY"
		wait(1)
		script.Parent.Text = "ðŸŽƒDawgHubðŸŽƒ | Beta | EASY."
		wait(1)
		script.Parent.Text = "ðŸŽƒDawgHubðŸŽƒ | Beta | EASY.G"
		wait(1)
		script.Parent.Text = "ðŸŽƒDawgHubðŸŽƒ | Beta | EASY.GG"
		wait(1)
		script.Parent.Text = ""
		wait(1)
		script.Parent.Text = "ðŸŽƒDawgHubðŸŽƒ | Beta | EASY.GG"
		wait(1)
		script.Parent.Text = ""
	end
end
coroutine.wrap(GJBB_fake_script)()
local function REVR_fake_script() -- AutoParry.LocalScript 
	local script = Instance.new('LocalScript', AutoParry)

	script.Parent.MouseButton1Click:Connect(function()
		getgenv().ScriptConfig = {
			-- Distance in stud before the automatic triggers,
			-- you may change even after you have ran the script if you desire!
			-- Just be sure to REMOVE the loadstring, or else you will face some... issues
			DistanceBeforeParry = 48,
		}
		local plyrs = game.Players
		local plyr = plyrs.LocalPlayer
		local hum = plyr.Character:FindFirstChildOfClass("Humanoid")
		if hum.Health == 0.5 then
			hum.Health = 100
		end
		local LocalPlayer = game:GetService("Players").LocalPlayer
		game:GetService("Workspace").Balls.ChildAdded:Connect(function(child)
			local trackTask = task.spawn(function()
				local ball = child
				while task.wait() do
					if string.find(ball.Color.Name:lower() or ball.BrickColor.Name:lower(), "red") then
						while LocalPlayer:DistanceFromCharacter(ball.CFrame.Position)
							> getgenv().ScriptConfig.DistanceBeforeParry do
							task.wait()
						end
						local ReplicatedStorage = game:GetService("ReplicatedStorage")
						ReplicatedStorage.Remotes.ParryButtonPress:Fire()
						ReplicatedStorage.Remotes.ParryButtonPress:Fire()
					end
				end
			end)
			child.Destroying:Connect(function()
				task.cancel(trackTask)
			end)
		end)
	end)
end
coroutine.wrap(REVR_fake_script)()
local function VCID_fake_script() -- SpeedUp.LocalScript 
	local script = Instance.new('LocalScript', SpeedUp)

	script.Parent.MouseButton1Click:Connect(function()
		local plyr = game.Players.LocalPlayer
		local char = plyr.Character
		local hum = char:FindFirstChildOfClass("Humanoid")
		hum.WalkSpeed = 80
	end)
end
coroutine.wrap(VCID_fake_script)()
local function DWFE_fake_script() -- AntiBan.Script 
	local script = Instance.new('Script', AntiBan)

	script.Parent.MouseButton1Click:Connect(function()
		while true do
			wait(2)
			if game.ReplicateStorage.Ban:FireServer() then
				game.ReplicatedStorage.Unban:FireServer()
				game.ReplicatedStorage.Unban:FireAllClients()
				local method = "(u.ban[fire=false]\nu.unban[fire=true])"
				print(method)
			end
		end
	end)
end
coroutine.wrap(DWFE_fake_script)()
local function BGSU_fake_script() -- HeadLockBall.LocalScript 
	local script = Instance.new('LocalScript', HeadLockBall)

	script.Parent.MouseButton1Click:Connect(function()
		local ball = game.Workspace.Balls
		if string.find(ball.BrickColor.Name:lower(), "red") then
			local plyrs = game.Players
			local plyr = plyrs.LocalPlayer
			local char = plyr.Character
			char.Position = ball
			char.PrimaryPart.Position = ball
		end
	end)
end
coroutine.wrap(BGSU_fake_script)()
